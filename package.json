  },
  "homepage": "https://mysterycommand.github.io/pxctrl",
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.5",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.0.5",
    "gulp": "^3.9.1",
