{
  "name": "pxctrl",
  "version": "1.0.0",
  "description": "This is an experiment in creating a system for automatically generating documentation and/or previews for other interactive projects.",
  "scripts": {
    "build": "webpack --config=config/webpack/production.babel.js && gulp",
    "clean": "rm -rf deploy/ public/",
    "deploy": "gulp deploy",
    "start": "webpack-dev-server --config=config/webpack/development.babel.js --port=3000 --inline --hot",
    "test": "mocha --compilers js:babel-register './test/**/*.test.js'"
  },
